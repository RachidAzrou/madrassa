{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "framework": null,
  "routes": [
    { "src": "/api/(.*)", "dest": "/api-vercel/index.js" },
    { "src": "/(.*\\.(js|css|png|jpg|gif|svg|ico))", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}